---
title: Ошибка 404
layout: default.hbs
---

<div class="page__wrapper" id="wrapper">

	{{> header }}

	<div class="brand-box brand-box--has-label">
		<div class="page__center">
			<div class="page__inner">
				<div class="brand-box__inner">
					<div class="brand-box__image">
						<img src="img/barsgroup.svg" alt="" width="151" height="63">
					</div>
					<div class="brand-box__desc">
						<div class="page__label page__label--small">Ошибка</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="page__center">
		<div class="page__inner">

			<div class="clearfix">
				<div class="page__col-side">

				</div>
				<div class="page__col-main">
					<div class="error">
						<img src="img/error/404.svg" alt="" class="error__image">

						<div class="error__content">
							<h1 class="error__message">
								Вы попали на страницу, которой не существует
							</h1>
							<p class="error__dont-panic">
								Не волнуйтесь, мы вам поможем
							</p>

							<a href="#" class="error__button button-aurora js-hover-wings">
								<span class="button-aurora__text">
									На главную
								</span>
								<i class="arrow-ciara button-aurora__icon">
									<span></span>
								</i>
							</a>
						</div>

					</div>


				</div>
			</div>

		</div>
	</div>



	{{> footer }}

</div>

{{> nav-mobile }}